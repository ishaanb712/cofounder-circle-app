{
  "name": "startupconnect-frontend",
  "version": "1.0.0",
  "description": "Frontend for StartupConnect platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:https": "next dev --experimental-https",
    "build": "next build",
